public class Solution {
    public IList<bool> PrefixesDivBy5(int[] nums) {
        bool[] retArr = new bool[nums.Length];
        int remainder = 0;

        for (int i = 0; i < nums.Length; i++) {
            remainder = (remainder * 2 + nums[i]) % 5; // Update remainder for the current prefix
            retArr[i] = (remainder == 0);// Check divisibility
        }

        return retArr;
    }
}
